{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**"]
        },
        "lint": {},
        "format": {},
        "test": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "deploy": {
            "dependsOn": ["build", "test", "lint"]
        },
        "codegen": {},
        "start": {
            "dependsOn": ["build"]
        }
    }
}
