{
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "npm run db && turbo dev",
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "db": "./scripts/docker_db",
        "seed": "npm run db && npx prisma db seed",
        "migrate:dev": "npm run db && npx prisma migrate dev"
    },
    "devDependencies": {
        "eslint": "^8.57.0",
        "prettier": "^3.2.5",
        "turbo": "^2.1.3"
    },
    "name": "with-vite",
    "packageManager": "npm@10.8.2",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "dependencies": {
        "@prisma/client": "^5.20.0"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}