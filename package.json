{
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "npm run db && npm run m:run && turbo dev",
        "lint": "turbo lint",
        "test": "turbo test",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
        "deploy": "turbo deploy",
        "db": "./scripts/docker_db",
        "m:create": "npm run db && npx prisma migrate dev",
        "m:run": "npm run db && npx prisma migrate deploy",
        "m:seed": "npm run db && npx prisma db seed",
        "generate-prisma-client": "npx prisma generate",
        "codegen": "npm run db && npm run generate-prisma-client && turbo codegen && ./scripts/generate_dto"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.14.0",
        "@prisma/client": "^5.21.1",
        "@types/node": "^22.7.5",
        "api": "^0.0.1",
        "eslint": "^8.57.0",
        "prettier": "^3.2.5",
        "prisma": "^5.21.1",
        "ts-node": "^10.9.2",
        "turbo": "^2.1.3",
        "typescript": "^5.6.3"
    },
    "name": "with-vite",
    "packageManager": "npm@10.8.2",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "prisma": {
        "seed": "ts-node prisma/seeds/seed.ts"
    }
}
